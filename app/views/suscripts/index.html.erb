<h1>Listing suscripts</h1>

<table>
  <tr>
    <th>User</th>
    <th>Suscription type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @suscripts.each do |suscript| %>
  <tr>
    <td><%= suscript.user_id %></td>
    <td><%= suscript.suscription_type %></td>
    <td><%= link_to 'Show', suscript %></td>
    <td><%= link_to 'Edit', edit_suscript_path(suscript) %></td>
    <td><%= link_to 'Destroy', suscript, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Suscript', new_suscript_path %>
